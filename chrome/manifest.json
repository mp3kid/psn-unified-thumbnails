{
  "manifest_version": 3,
  "name": "Unified PSN Thumbnails",
  "version": "1.3.0",
  "description": "Unifies the sizes of game thumbnails when viewing PSNProfiles",
  "content_scripts": [
    {
      "matches": [
        "https://psnprofiles.com/*"
      ],
      "css": [
        "style/unified-thumbnails.css"
      ],
      "js": [
        "script/unified-thumbnails.js",
        "script/remove-empty-trophy-sets.js",
        "script/add-impossible-rarities.js"
      ]
    }
  ]
}